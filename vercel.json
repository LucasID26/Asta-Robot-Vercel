{
  "version": 2,
  "builds": [
    {
      "src": "fast.py",
      "use": "@vercel/python"
    },
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/main",
      "dest": "main.py"
    },
    {
      "src": "/(.*)",
      "dest": "/fast.py"
    }
  ]
}
